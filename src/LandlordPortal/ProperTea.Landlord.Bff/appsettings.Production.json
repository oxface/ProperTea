{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "Authority": "#{Identity.Authority}#",
    "Audience": "#{Identity.Audience}#"
  },
  "Cors": {
    "AllowedOrigins": [
      "#{Frontend.BaseUrl}#"
    ]
  },
  "OrganizationCreation": {
    "SynchronousMode": true,
    "TimeoutSeconds": 45,
    "PollingIntervalMs": 2000
  }
}

